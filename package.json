{
  "name": "design-only",
  "main": "expo-router/entry",
  "version": "1.0.0",
  "type": "commonjs",
  "scripts": {
    "start": "env-cmd -f .env.development expo start --dev-client",
    "start:dev": "env-cmd -f .env.development expo start --dev-client",
    "start:demo": "env-cmd -f .env.demo expo start --dev-client",
    "start:live": "env-cmd -f .env.production expo start --dev-client",
    "expo-go": "npx expo start --clear",
    "reset-project": "node ./scripts/reset-project.js",
    "android": "env-cmd -f .env.development expo start --android --dev-client",
    "android:demo": "env-cmd -f .env.demo expo start --android --dev-client",
    "android:live": "env-cmd -f .env.production expo start --android --dev-client",
    "ios": "env-cmd -f .env.development expo start --ios --dev-client",
    "ios:demo": "env-cmd -f .env.demo expo start --ios --dev-client",
    "ios:live": "env-cmd -f .env.production expo start --ios --dev-client",
    "web": "env-cmd -f .env.development expo start --web",
    "docs": "markdown-serve ./docs",
    "docs:open": "markdown-serve ./docs --open",
    "setup": "node scripts/setup-project.js",
    "release": "node scripts/release-version.js",
    "flag-audit": "node scripts/flag-audit.js",
    "flag-audit:strict": "node scripts/flag-audit.js --strict",
    "flag-audit:usage": "node scripts/flag-audit.js --check-usage",
    "flag-cleanup": "node scripts/flag-cleanup.js",
    "flag-cleanup:pr": "node scripts/flag-cleanup.js --create-pr",
    "type-check": "tsc --noEmit",
    "fix:imports": "node scripts/fix-imports.js",
    "clear-cache": "rm -rf node_modules/.cache && rm -rf $TMPDIR/metro-* && watchman watch-del-all 2>/dev/null || true",
    "restart": "npm run clear-cache && env-cmd -f .env.development expo start --dev-client --clear"
  },
  "dependencies": {
    "@babel/runtime": "^7.27.1",
    "@expo/ngrok": "^4.1.3",
    "@expo/vector-icons": "^14.1.0",
    "@octokit/rest": "^21.1.1",
    "@react-native-async-storage/async-storage": "1.23.1",
    "@react-native-community/netinfo": "^11.4.1",
    "@react-native-community/slider": "^4.5.5",
    "@react-native-picker/picker": "2.9.0",
    "@react-navigation/native": "^7.0.14",
    "@react-navigation/native-stack": "^7.2.0",
    "@supabase/supabase-js": "^2.49.4",
    "@types/react-dom": "~18.3.1",
    "ajv": "^8.17.1",
    "eventemitter3": "^5.0.1",
    "expo": "~52.0.43",
    "expo-application": "~6.0.2",
    "expo-av": "~15.0.2",
    "expo-blur": "~14.0.3",
    "expo-constants": "~17.0.8",
    "expo-doctor": "^1.13.2",
    "expo-document-picker": "~13.0.3",
    "expo-font": "~13.0.4",
    "expo-image-picker": "~16.0.6",
    "expo-linear-gradient": "~14.0.2",
    "expo-linking": "~7.0.5",
    "expo-router": "~4.0.20",
    "expo-splash-screen": "^0.29.24",
    "expo-status-bar": "~2.0.1",
    "i18next": "^24.2.3",
    "react": "18.3.1",
    "react-i18next": "^15.4.1",
    "react-native": "0.76.9",
    "react-native-safe-area-context": "4.12.0",
    "react-native-screens": "~4.4.0",
    "react-native-url-polyfill": "^2.0.0",
    "react-native-web": "~0.19.13",
    "react-native-webview": "13.12.5",
    "zod": "^3.25.67",
    "zustand": "^5.0.3"
  },
  "devDependencies": {
    "@babel/core": "^7.25.2",
    "@expo/metro-config": "^0.19.0",
    "@react-native-community/cli-tools": "^18.0.0",
    "@types/react": "~18.3.12",
    "@typescript-eslint/eslint-plugin": "^8.30.1",
    "@typescript-eslint/parser": "^8.30.1",
    "babel-plugin-module-resolver": "^5.0.2",
    "chalk": "^4.1.2",
    "commander": "^9.5.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.57.1",
    "eslint-import-resolver-alias": "^1.1.2",
    "eslint-import-resolver-typescript": "^4.3.4",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-jsdoc": "^46.10.1",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^4.6.2",
    "glob": "^11.0.2",
    "husky": "^8.0.3",
    "lint-staged": "^13.3.0",
    "metro": "^0.81.0",
    "metro-resolver": "^0.81.5",
    "prettier": "^2.8.8",
    "ts-prune": "^0.10.3",
    "typescript": "^5.3.3"
  },
  "private": true
}
